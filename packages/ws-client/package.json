  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch --watch.include=src/**",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "birpc": "^0.2.3",
    "flatted": "^3.2.7",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "rollup": "^2.79.1"
  }
}
