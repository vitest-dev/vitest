{
  "name": "@vitest/browser",
  "type": "module",
  "version": "0.22.0",
  "description": "Browser running for Vitest",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitest-dev/vitest.git",
    "directory": "packages/browser"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "stubs"
  ],
  "scripts": {
    "build": "rimraf dist && pnpm build:node && pnpm build:client && pnpm copy",
    "build:client": "vite build",
    "build:node": "rollup -c",
    "dev:client": "vite build --watch",
    "dev:node": "rollup -c --watch --watch.include=node/**",
    "dev": "rimraf dist && run-p dev:node dev:client",
    "copy": "esno scripts/copy-ui-to-browser.ts"
  },
  "dependencies": {
    "local-pkg": "^0.4.2",
    "mlly": "^0.5.12",
    "modern-node-polyfills": "0.0.9",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "sirv": "^2.0.2"
  },
  "devDependencies": {
    "@types/ws": "^8.5.3",
    "@vitest/ws-client": "workspace:*",
    "picocolors": "^1.0.0",
    "rollup": "^2.77.3",
    "vitest": "workspace:*"
  }
}
